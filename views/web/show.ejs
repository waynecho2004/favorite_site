<h4><%= data.name %> Favorite Links</h4>
<a class="btn btn-primary" data-toggle="collapse" href="#newlinkForm" role="button" aria-expanded="false" aria-controls="collapseExample">
  Add New Link
</a>

<form class="collapse" id='newlinkForm' action="/web/<%= data.id %>/urls" method="POST">
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" class="form-control" id="title" name="title" />
  </div>
  <div class="form-group">
    <label for="url">Link</label>
    <input type="text" class="form-control" id="url" name="url" />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<ul>
    <% data.urls.forEach(url => { %>
    <li>
      <a href="<%=url.url%>" target="_blank"><%= url.title %></a>
    </li>
    <% }) %>
</ul>